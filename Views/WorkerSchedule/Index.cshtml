@model IEnumerable<PSA_MVC_V2.Models.Database.WorkerSchedule>

@{
    ViewData["Title"] = "Darbuotojų grafikai";
}

<h1>Darbuotojų grafikai</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SType)
            </th>
            <th>
                @Html.DisplayName("Date From-To")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkTimeTable.From)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkTimeTable.To)
            </td>
            <td>
                <button>Užpildyti laiko grafiką</button>
            </td>
        </tr>
}
    </tbody>
</table>
